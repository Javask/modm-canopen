import subprocess
from pathlib import Path

def init(module):
    module.name = "device:test"
    module.description = "Classes to test the implementation on a host"


def prepare(module, options):
    module.depends("modm-canopen:device")
    return True


def build(env):
    env.outbasepath = "modm-canopen/src/modm-canopen"
    env.copy(".")
